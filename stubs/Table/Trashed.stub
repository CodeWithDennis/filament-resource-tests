it('cannot display trashed records by default', function () {
    $records = {{ MODEL_SINGULAR_NAME }}::factory(3)->create();

    $trashedRecords = {{ MODEL_SINGULAR_NAME }}::factory(6)->trashed()->create();

    livewire(List{{ MODEL_PLURAL_NAME }}::class){{ RESOURCE_TABLE_HAS_DEFERRED_LOADING_LOAD_TABLE_METHOD }}
        ->assertCanSeeTableRecords($records)
        ->assertCanNotSeeTableRecords($trashedRecords)
        ->assertCountTableRecords(3);
})->group('table');
